package com.flightapp.commonmodule.utility;

import com.google.gson.Gson;

public class JsonUtil {
	private JsonUtil() {}
	
	public static String toJson(Object aClass) {
		return new Gson().toJson(aClass);	
	}
	
	public static Object fromJson(String jsonString,Class<T> aClass) {
		return new Gson().fromJson(jsonString, aClass);
		
	}

}
